generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model animals {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  v       Int?     @map("__v")
  adopted Boolean?
  age     Int
  breed   String
  colors  String[]
  gender  String
  name    String
  species String?
}
